{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "entry",
                "targets": {
                    "browsers": [
                        ">0.25%",
                        "not ie 11",
                        "not op_mini all"
                    ]
                },
                "forceAllTransforms": true
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        ["@babel/plugin-proposal-object-rest-spread", {}, "spread"],
        ["transform-class-properties", {}, "class"]
    ],
    "ignore": [
        "node_modules"
    ]
}