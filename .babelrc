{
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "browsers": [
                        ">0.25%",
                        "not ie 11",
                        "not op_mini all"
                    ]
                },
                "forceAllTransforms": true
            }
        ],
        "@babel/react"
    ],
    "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties"
    ],
    "ignore": [
        "node_modules"
    ]
}