	"scripts": {
		"build": "yarn build:clean && yarn build:dist",
		"build:clean": "rimraf build",
		"build:dist": "NODE_ENV='production' webpack --mode production --config=webpack/production.config.js",
		"build:rollup": "yarn clean && NODE_ENV=production rollup --config rollup.config.js app/index.js",
		"dev": "yarn dev:build",
		"dev:build": "NODE_ENV=development parcel app/index.html",
		"lint": "eslint --config ./.eslinrc.yml --fix src app",
		"test": "yarn test:clean && yarn test:jest",
		"test:clean": "rimraf build_artifacts",
		"test:jest": "NODE_ENV=test jest --config jest.config.js",
		"test:watch": "yarn test --watch",
		"test:cov": "yarn test --converage",
		"test:watch:cov": "yarn test:watch --coverage"
	},
